{
    "name": "imagine-ui",
    "version": "1.0.0",
    "description": "Imagine UI",
    "author": "Compute Essentials <corporate@compute-essentials.com>",
    "repository": {
        "type": "git",
        "url": "https://github.com/tonyobanon/project_panther_fe"
    },
    "license": "MIT",
    "git": {
        "hash": "%GIT_HASH%",
        "ref": "%GIT_REF%",
        "url": "%GIT_URL%"
    },
    "scripts": {
        "install-deps": "npm install && npm install --only=dev",
        "start": "nodemon server/index.js",
        "build-archive": "gulp dist && node lib/model-factories/build-archive.js",
        "build": "gulp dist",
        "build-watch": "npm run build && buildArchive=true gulp watch",
        "templates": "gulp dist-templates",
        "templates-all": "gulp dist-templates && npm run build-archive && buildArchive=true gulp watch-templates",
        "lint": "eslint --ext js --fix src/ tasks/",
        "pretest": "npm run lint",
        "test": "jest --runInBand --forceExit --coverage"
    },
    "dependencies": {
        "@babel/core": "^7.11.4",
        "@ampproject/worker-dom": "^0.27.3",
        "browserify": "^16.5.0",
        "csso": "^4.0.3",
        "escodegen": "^1.14.3",
        "esprima": "^4.0.1",
        "esprima-walk": "^0.1.0",
        "faker": "^4.1.0",
        "fancy-log": "^1.3.3",
        "gulp": "^4.0.2",
        "gulp-babel": "^8.0.0",
        "gulp-imagemin": "^6.1.1",
        "gulp-plumber": "^1.2.1",
        "gulp-rename": "^1.4.0",
        "gulp-sass": "^4.0.2",
        "gulp-sourcemaps": "^2.6.5",
        "gulp-uglify": "^3.0.2",
        "gulp-watch": "^5.0.1",
        "gulplog": "^1.0.0",
        "handlebars": "^4.7.6",
        "htmlparser2": "^5.0.1",
        "hyntax": "^1.1.9",
        "import-fresh": "^3.2.1",
        "jsdom": "^16.4.0",
        "lodash": "4.17.20",
        "normalize-scss": "7.0.1",
        "quicktype-core": "^6.0.68",
        "require-dir": "^1.2.0",
        "require-from-string": "^2.0.2",
        "restify": "^8.4.0",
        "run-sequence": "^2.2.1",
        "shelljs": "^0.8.4",
        "through2": "^3.0.1",
        "uglify-js": "^3.10.1",
        "vinyl-buffer": "^1.0.1",
        "vinyl-source-stream": "^2.0.0",
        "xml-js": "^1.6.11"
    },
    "devDependencies": {
        "@babel/plugin-proposal-class-properties": "^7.5.5",
        "@babel/plugin-proposal-private-methods": "^7.12.1",
        "@babel/preset-env": "^7.6.3",
        "babel-eslint": "11.0.0-beta.2",
        "eslint": "^7.13.0",
        "eslint-config-airbnb-base": "^13.2.0",
        "eslint-plugin-import": "^2.18.2",
        "jest": "^24.9.0",
        "nodemon": "^1.19.4"
    },
    "jest": {
        "verbose": true,
        "forceExit": true,
        "collectCoverage": false,
        "coverageThreshold": {
            "global": {
                "branches": 100,
                "functions": 100,
                "lines": 100,
                "statements": 100
            }
        },
        "testEnvironment": "browser"
    }
}
