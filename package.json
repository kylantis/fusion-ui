{
    "name": "imagine-ui",
    "version": "1.0.0",
    "description": "Imagine UI",
    "author": "Compute Essentials <corporate@compute-essentials.com>",
    "repository": {
        "type": "git",
        "url": "https://github.com/tonyobanon/project_panther_fe"
    },
    "license": "MIT",
    "git": {
        "hash": "%GIT_HASH%",
        "ref": "%GIT_REF%",
        "url": "%GIT_URL%"
    },
    "scripts": {
        "start-server": "nodemon server/index.js",
        "build-archive": "APP_ID=platform node lib/model-factories/build-archive.js",
        "build": "APP_ID=platform gulp dist && npm run build-archive",
        "scripts": "gulp compile-scripts",
        "watch": "APP_ID=platform gulp watch",
        "watch-build-archive": "BUILD_ARCHIVE=true npm run watch",
        "build-watch": "npm run build && npm run watch",
        "build-all": "APP_ID=platform gulp distAll && npm run build-archive",
        "lint": "eslint --ext js --fix src/ tasks/",
        "pretest": "npm run lint",
        "test": "jest --runInBand --forceExit --coverage"
    },
    "dependencies": {
        "@ampproject/worker-dom": "^0.27.3",
        "@babel/core": "^7.20.2",
        "@handlebars/parser": "^2.1.0",
        "acorn": "^8.7.0",
        "acorn-walk": "^8.2.0",
        "ajv": "^8.6.3",
        "astring": "^1.8.1",
        "csso": "^4.0.3",
        "fs-extra": "^10.0.0",
        "handlebars": "^4.7.6",
        "html-validate": "^4.6.0",
        "hyntax": "^1.1.9",
        "import-fresh": "^3.2.1",
        "jsdom": "^20.0.3",
        "line-column": "^1.0.2",
        "lodash": "^4.17.21",
        "normalize-scss": "7.0.1",
        "quicktype-core": "^6.0.70",
        "randomstring": "1.1.5",
        "uglify-js": "^3.17.4",
        "xml-js": "^1.6.11"
    },
    "devDependencies": {
        "@babel/plugin-proposal-class-properties": "^7.5.5",
        "@babel/plugin-proposal-private-methods": "^7.12.1",
        "@babel/preset-env": "^7.6.3",
        "babel-eslint": "11.0.0-beta.2",
        "browserify": "^16.5.0",
        "buffer": "6.0.3",
        "esbuild": "0.16.12",
        "esbuild-plugin-external-global": "^1.0.1",
        "eslint": "7.10.0",
        "eslint-config-airbnb-base": "^14.2.1",
        "eslint-plugin-import": "^2.18.2",
        "gulp": "^4.0.2",
        "gulp-imagemin": "^6.1.1",
        "gulp-rename": "^1.4.0",
        "gulp-sass": "^5.0.0",
        "gulp-sourcemaps": "^2.6.5",
        "gulp-uglify": "^3.0.2",
        "gulplog": "^1.0.0",
        "jest": "^24.9.0",
        "nodemon": "^1.19.4",
        "require-dir": "^1.2.0",
        "require-from-string": "^2.0.2",
        "restify": "^9.0.0",
        "run-sequence": "^2.2.1",
        "sass": "1.43.4",
        "shelljs": "^0.8.4",
        "stream-browserify": "^3.0.0",
        "through2": "^3.0.1",
        "vinyl-buffer": "^1.0.1",
        "vinyl-source-stream": "^2.0.0"
    },
    "jest": {
        "verbose": true,
        "forceExit": true,
        "collectCoverage": false,
        "coverageThreshold": {
            "global": {
                "branches": 100,
                "functions": 100,
                "lines": 100,
                "statements": 100
            }
        },
        "testEnvironment": "browser"
    }
}
