{
    "name": "kylantis-ui-compiler",
    "version": "1.0.0",
    "description": "Imagine UI",
    "author": "Tony <tonyobanon@gmail.com>",
    "repository": {
        "type": "git",
        "url": "https://github.com/tonyobanon/project_panther_fe"
    },
    "license": "MIT",
    "git": {
        "hash": "%GIT_HASH%",
        "ref": "%GIT_REF%",
        "url": "%GIT_URL%"
    },
    "scripts": {
        "start-server": "nodemon server/index.js",
        "build-archive": "APP_ID=platform node lib/model-factories/build-archive.js",
        "build": "APP_ID=platform gulp build && npm run build-archive",
        "scripts": "gulp compile-scripts",
        "watch": "APP_ID=platform gulp watch",
        "watch-build-archive": "BUILD_ARCHIVE=true npm run watch",
        "build-watch": "npm run build && npm run watch",
        "compile-components": "APP_ID=platform gulp compile-components",
        "compile-component": "APP_ID=platform gulp compile-component",
        "lint": "eslint --ext js --fix src/ tasks/",
        "pretest": "npm run lint",
        "test": "jest --runInBand --forceExit --coverage"
    },
    "dependencies": {
        "@babel/core": "^7.24.0",
        "@handlebars/parser": "^2.1.0",
        "acorn": "^8.8.2",
        "astring": "^1.8.4",
        "fs-extra": "^11.2.0",
        "handlebars": "^4.7.8",
        "html-validate": "^4.6.0",
        "hyntax": "^1.1.9",
        "import-fresh": "^3.2.1",
        "jsdom": "^20.0.3",
        "line-column": "^1.0.2",
        "lodash": "^4.17.21",
        "lz-string": "^1.5.0",
        "normalize-scss": "7.0.1",
        "quicktype-core": "^21.0.2",
        "randomstring": "1.1.5",
        "uglify-js": "^3.18.0",
        "xml-js": "^1.6.11"
    },
    "devDependencies": {
        "@babel/plugin-proposal-class-properties": "^7.18.6",
        "@babel/plugin-proposal-private-methods": "^7.18.6",
        "@babel/preset-env": "^7.6.3",
        "babel-eslint": "11.0.0-beta.2",
        "brotli-wasm": "^3.0.1",
        "browserify": "^16.5.0",
        "buffer": "6.0.3",
        "esbuild": "^0.23.0",
        "eslint": "7.10.0",
        "eslint-config-airbnb-base": "^14.2.1",
        "eslint-plugin-import": "^2.18.2",
        "fake-indexeddb": "6.0.0",
        "gulp": "^4.0.2",
        "gulp-if": "^3.0.0",
        "gulp-imagemin": "^6.1.1",
        "gulp-rename": "^1.4.0",
        "gulp-sass": "^5.0.0",
        "gulp-sourcemaps": "^2.6.5",
        "gulp-uglify": "^3.0.2",
        "gulplog": "^1.0.0",
        "jest": "^24.9.0",
        "mime-types": "^2.1.35",
        "nodemon": "^1.19.4",
        "require-dir": "^1.2.0",
        "require-from-string": "^2.0.2",
        "restify": "^11.1.0",
        "restify-errors": "^8.0.2",
        "run-sequence": "^2.2.1",
        "sass": "1.43.4",
        "shelljs": "^0.8.4",
        "stream-browserify": "^3.0.0",
        "stream-combiner2": "^1.1.1",
        "through2": "^3.0.1",
        "vinyl-buffer": "^1.0.1",
        "vinyl-source-stream": "^2.0.0"
    },
    "jest": {
        "verbose": true,
        "forceExit": true,
        "collectCoverage": false,
        "coverageThreshold": {
            "global": {
                "branches": 100,
                "functions": 100,
                "lines": 100,
                "statements": 100
            }
        },
        "testEnvironment": "browser"
    }
}
