{
  "enableTypeMerging": true,
  "collections": {
    "Sentence.parts": {
      "singularType": "Part"
    },
    "items": {
      "minLength": 5,
      "singularType": "Item"
    },
    "items.$_.summary.parts": {
      "minLength": 5,
      "singularType": {
        "$ref": "Sentence.parts"
      }
    },
    "items.$_.description.parts": {
      "minLength": 1,
      "singularType": {
        "$ref": "Sentence.parts"
      }
    },
    "items.$_.title.left": {
      "minLength": 1
    },
    "items.$_.title.right": {
      "minLength": 1
    },
    "items.$_.properties": {
      "minLength": 2,
      "defaultKeys": [
        "Name",
        "Related To",
        "From Address",
        "To Address",
        "Location",
        "Attendees",
        "When"
      ]
    }
  },
  "scalars": {
    "Sentence.parts_$.link": {
      "type": "Link"
    },
    "Sentence.parts_$.text": {
      "type": "String"
    },
    "items.$_.expanded": {
      "type": "Boolean",
      "defaultValue": false
    },
    "items.$_.expandable": {
      "type": "Boolean",
      "defaultValue": true
    },
    "items.$_.assistiveText": {
      "type": "String",
      "defaults": []
    },
    "items.$_.lineColor": {
      "type": "String",
      "allowed": "cssColor",
      "defaults": [
        "PowderBlue",
        "PaleGreen",
        "SteelBlue",
        "MediumVioletRed",
        "PeachPuff",
        "Peru",
        "Plum"
      ]
    },
    "items.$_.icon": {
      "type": "Icon",
      "isNull": false
    },
    "items.$_.actions": {
      "type": "Menu",
      "isNull": false
    },
    "items.$_.summary.parts_$.link": {
      "type": {
        "$ref": "Sentence.parts_$.link"
      },
      "isNull": false
    },
    "items.$_.summary.parts_$.text": {
      "type": {
        "$ref": "Sentence.parts_$.text"
      },
      "defaults": [
        "You",
        "the scheduled meeting",
        "created a task with",
        "Charlie Gomez",
        "logged a call with",
        "Adam Chan",
        "share the presentation",
        "emailed",
        "John Doe",
        "created an event with",
        "Aida Lee",
        "and 5 others",
        "invites you to a hurdle",
        "brainstorming session today",
        "monotonectally morph effective ideas",
        "interactively grow prospective services",
        "intrinsically customize cooperative architectures",
        "synergistically matrix intermandated paradigms",
        "quickly develop holistic portals",
        "credibly promote front-end outsourcing",
        "completely communicate next-generation platforms",
        "assertively negotiate compelling e-markets",
        "dramatically integrate standards compliant solutions",
        "completely reinvent front-end potentialities",
        "seamlessly implement innovative processes",
        "enthusiastically actualize cutting-edge e-tailers",
        "proactively exploit B2B solutions"
      ]
    },
    "items.$_.description.parts_$.link": {
      "type": {
        "$ref": "Sentence.parts_$.link"
      },
      "isNull": true
    },
    "items.$_.description.parts_$.text": {
      "type": {
        "$ref": "Sentence.parts_$.text"
      },
      "defaults": [
        "Need to finalize proposals and brand details before the meeting",
        "Create the deployments assets prior to production validation and release"
      ]
    },
    "items.$_.title.main": {
      "type": "Link",
      "isNull": false
    },
    "items.$_.title.left_$": {
      "type": "TextCompanion",
      "isNull": false,
      "defaults": [
        "CheckboxBase"
      ]
    },
    "items.$_.title.right_$": {
      "type": "TextCompanion",
      "isNull": false
    },
    "items.$_.date": {
      "type": "String",
      "defaults": [
        "9:00am | 3/20/17"
      ]
    },
    "items.$_.properties.$_": {
      "type": "Link",
      "isNull": false
    }
  },
  "sample": {
    "nullifyRandomly": [
      "items.$_.summary.parts_$.link"
    ]
  },
  "sharedTypeNames": {
    "SharedType0": "Sentence"
  },
  "escapeDynamicHtml": false
}