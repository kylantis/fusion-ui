{
  "enableTypeMerging": true,
  "collections": {
    "items": {
      "minLength": 5,
      "singularType": "Item"
    },
    "items.$_.summary.parts": {
      "minLength": 5,
      "singularType": {
        "$ref": "Sentence.parts"
      }
    },
    "items.$_.description.parts": {
      "minLength": 1,
      "singularType": {
        "$ref": "Sentence.parts"
      }
    },
    "Sentence.parts": {
      "singularType": "Part"
    }
  },
  "scalars": {
    "items.$_.expanded": {
      "type": "Boolean",
      "defaultValue": false
    },
    "items.$_.expandable": {
      "type": "Boolean",
      "defaultValue": true
    },
    "items.$_.assistiveText": {
      "type": "String",
      "defaults": []
    },
    "items.$_.lineColor": {
      "type": "String",
      "allowed": "cssColor",
      "defaults": [
        "PowderBlue",
        "PaleGreen",
        "SteelBlue",
        "MediumVioletRed",
        "PeachPuff",
        "Peru",
        "Plum"
      ]
    },
    "items.$_.checkable": {
      "type": "Boolean",
      "defaultValue": true
    },
    "items.$_.icon": {
      "type": "Icon",
      "isNull": false
    },
    "items.$_.actions": {
      "type": "Menu",
      "isNull": false
    },
    "items.$_.summary.parts_$.link": {
      "type": {
        "$ref": "Sentence.parts_$.link"
      },
      "defaults": [
        "http://www.google.com"
      ]
    },
    "items.$_.summary.parts_$.text": {
      "type": {
        "$ref": "Sentence.parts_$.text"
      },
      "defaults": [
        "You",
        "the scheduled meeting",
        "created a task with",
        "Charlie Gomez",
        "logged a call with",
        "Adam Chan",
        "share the presentation",
        "emailed",
        "John Doe",
        "created an event with",
        "Aida Lee",
        "and 5 others",
        "invites you to a hurdle",
        "brainstorming session today"
      ]
    },
    "items.$_.description.parts_$.link": {
      "type": {
        "$ref": "Sentence.parts_$.link"
      },
      "defaults": [
        null
      ]
    },
    "items.$_.description.parts_$.text": {
      "type": {
        "$ref": "Sentence.parts_$.text"
      },
      "defaults": [
        "Need to finalize proposals and brand details before the meeting",
        "Create the deployments assets prior to production validation and release"
      ]
    },
    "Sentence.parts_$.link": {
      "type": "String"
    },
    "Sentence.parts_$.text": {
      "type": "String"
    }
  },
  "sample": {
    "nullifyRandomly": [
      "items.$_.summary.parts_$.link"
    ]
  },
  "sharedTypeNames": {
    "SharedType0": "Sentence"
  },
  "escapeDynamicHtml": false
}