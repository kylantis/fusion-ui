{
  "superclass": null,
  "collections": {
    "groups": {
      "minLength": 1,
      "singularType": "Group"
    },
    "groups_$.items": {
      "minLength": 3,
      "singularType": "Item"
    }
  },
  "scalars": {
    "groups_$.title": {
      "type": "String"
    },
    "groups_$.items_$.title": {
      "type": "String",
      "defaults": [
        "Menu Item One",
        "Menu Item Two"
      ]
    },
    "label": {
      "type": "String",
      "defaults": []
    },
    "groups_$.role": {
      "type": "String",
      "allowed": [
        "presentation",
        "checkbox",
        "radio"
      ],
      "defaults": [
        "presentation"
      ]
    },
    "groups_$.items_$.identifier": {
      "type": "String",
      "defaults": []
    },
    "groups_$.checkIconPosition": {
      "type": "String",
      "allowed": "direction",
      "defaults": [
        "right"
      ]
    },
    "groups_$.items_$.level": {
      "type": "String",
      "allowed": [
        "error",
        "success",
        "warning"
      ]
    },
    "groups_$.required": {
      "type": "Boolean",
      "defaultValue": false
    },
    "size": {
      "type": "String",
      "allowed": "size",
      "defaults": [
        "small"
      ]
    },
    "isSubMenu": {
      "type": "Boolean"
    },
    "direction": {
      "type": "String",
      "allowed": "direction"
    },
    "groups_$.items_$.subMenuDirection": {
      "type": "String",
      "allowed": "direction"
    },
    "coordinates.Y": {
      "type": "Number"
    },
    "coordinates.X": {
      "type": "Number"
    },
    "groups_$.items_$.leftIcon": {
      "type": "Icon",
      "isNull": true
    },
    "groups_$.items_$.rightIcon": {
      "type": "Icon",
      "isNull": true
    },
    "groups_$.items_$.subMenu": {
      "type": "Menu",
      "isNull": true
    }
  }
}