{
  "collections": {
    "groups": {
      "minLength": 1,
      "singularType": "Group"
    },
    "groups_$.items": {
      "minLength": 6,
      "singularType": "Item"
    }
  },
  "scalars": {
    "label": {
      "type": "String",
      "defaults": []
    },
    "size": {
      "type": "String",
      "allowed": [
        "xx-small",
        "x-small",
        "small",
        "medium",
        "large"
      ],
      "defaults": [
        null
      ]
    },
    "isSubMenu": {
      "type": "Boolean",
      "clientOnly": true
    },
    "overlay": {
      "type": "Boolean"
    },
    "x": {
      "type": "String",
      "allowed": "positionX",
      "clientOnly": true
    },
    "y": {
      "type": "String",
      "allowed": "positionY",
      "clientOnly": true
    },
    "groups_$.title": {
      "type": "String"
    },
    "groups_$.role": {
      "type": "String",
      "allowed": [
        "presentation",
        "checkbox",
        "radio"
      ],
      "defaults": [
        "presentation"
      ]
    },
    "groups_$.checkIconX": {
      "type": "String",
      "allowed": "positionX"
    },
    "groups_$.required": {
      "type": "Boolean"
    },
    "groups_$.items_$.identifier": {
      "type": "String",
      "defaults": []
    },
    "groups_$.items_$.title": {
      "type": "String",
      "defaults": [
        "Menu Item One",
        "Menu Item Two"
      ]
    },
    "groups_$.items_$.feedbackState": {
      "type": "String",
      "allowed": "feedbackState",
      "defaults": [
        null
      ]
    },
    "groups_$.items_$.leftMargin": {
      "type": "Boolean",
      "clientOnly": true
    },
    "groups_$.items_$.rightMargin": {
      "type": "Boolean",
      "clientOnly": true
    },
    "groups_$.items_$.leftIcon": {
      "type": "Icon",
      "isNull": false
    },
    "groups_$.items_$.rightIcon": {
      "type": "Icon",
      "isNull": true
    },
    "groups_$.items_$.subMenu": {
      "type": "Menu",
      "isNull": true
    },
    "groups_$.items_$.subMenuX": {
      "type": "String",
      "allowed": "positionX"
    },
    "groups_$.items_$.subMenuY": {
      "type": "String",
      "allowed": "positionY"
    }
  },
  "events": {
    "click": {
      "parameters": {
        "identifier": {
          "type": "string"
        }
      }
    },
    "select": {
      "parameters": {
        "identifier": {
          "type": "string"
        }
      }
    },
    "unselect": {
      "parameters": {
        "identifier": {
          "type": "string"
        }
      }
    }
  },
  "behaviours": {
    "selectItem": {
      "parameters": {
        "identifier": {
          "type": "string"
        }
      }
    },
    "unselectItem": {
      "parameters": {
        "identifier": {
          "type": "string"
        }
      }
    },
    "renderSubMenu": {
      "parameters": {
        "identifier": {
          "type": "string"
        }
      }
    },
    "removeSubMenu": {
      "parameters": {
        "identifier": {
          "type": "string"
        }
      }
    }
  },
  "sample": {
    "count": 1,
    "nullifyRandomly": [
      "groups_$.items_$.leftIcon",
      "groups_$.items_$.feedbackState"
    ]
  },
  "escapeDynamicHtml": false,
  "hookAccessRate": 1
}