{
  "collections": {
    "items": {
      "minLength": 5,
      "singularType": "Item"
    }
  },
  "scalars": {
    "expandSingle": {
      "type": "Boolean",
      "defaultValue": false
    },
    "items_$.body": {
      "type": "componentRef",
      "defaults": [
        "Text"
      ]
    },
    "items_$.bodyPadding": {
      "type": "String",
      "allowed": "spacingType",
      "defaults": [
        "horizontal_medium"
      ]
    },
    "items_$.title": {
      "type": "String",
      "defaults": []
    }
  },
  "escapeDynamicHtml": false
}