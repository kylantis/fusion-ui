    "margin": {
      "type": "String",
      "allowed": "spacingType"
    },
    "displayOnlyControl": {
      "type": "Boolean",
      "defaultValue": false
    },
    "readonly": {
      "type": "Boolean",
      "defaultValue": false
    },
    "editable": {
      "type": "Boolean",
      "defaultValue": true
    },
    "inlineEdit": {
      "type": "Boolean",
      "defaultValue": true
    },
    "dependsOn": {
      "type": "FormElement",
      "isNull": true
    },
    "editing": {
      "type": "Boolean",
      "clientOnly": true
    }
