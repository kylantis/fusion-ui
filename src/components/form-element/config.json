{
  "collections": {},
  "scalars": {
    "name": {
      "type": "String"
    },
    "titleText": {
      "type": "String",
      "defaults": [
        "My FormElement Label"
      ]
    },
    "message": {
      "type": "String"
    },
    "titleIcon": {
      "type": "ButtonIcon",
      "isNull": false
    },
    "title": {
      "type": "Text",
      "isNull": false
    },
    "disabled": {
      "type": "Boolean",
      "defaultValue": false
    },
    "required": {
      "type": "Boolean",
      "defaultValue": false
    },
    "error": {
      "type": "Boolean",
      "defaultValue": false
    },
    "layoutType": {
      "type": "String",
      "allowed": "formLayoutType"
    },
    "padding": {
      "type": "String",
      "allowed": "spacingType"
    },
    "margin": {
      "type": "String",
      "allowed": "spacingType"
    },
    "controlOnly": {
      "type": "Boolean",
      "defaultValue": false
    },
    "readonly": {
      "type": "Boolean",
      "defaultValue": false
    },
    "editable": {
      "type": "Boolean",
      "defaultValue": true
    },
    "inlineEdit": {
      "type": "Boolean",
      "defaultValue": true
    },
    "dependsOn": {
      "type": "FormElement",
      "isNull": true
    },
    "dependsOnRef": {
      "type": "String",
      "isNull": true
    },
    "editing": {
      "type": "Boolean",
      "clientOnly": true
    }
  },
  "behaviours": {
    "setError": {
      "parameters": {
        "error": {
          "type": {
            "$ref": "error"
          }
        }
      }
    },
    "setMessage": {
      "parameters": {
        "message": {
          "type": {
            "$ref": "message"
          }
        }
      }
    }
  },
  "events": {
    "change": {
      "parameters": {
        "value": {
          "type": "string"
        }
      }
    },
    "activate": {
      "parameters": {
        "value": {
          "type": "string"
        }
      }
    }
  },
  "sample": {
    "nullifyRandomly": []
  },
  "escapeDynamicHtml": false
}