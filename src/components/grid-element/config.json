{
  "enableTypeMerging": false,
  "collections": {
    "size": {
      "minLength": 0
    },
    "order": {
      "minLength": 0
    }
  },
  "scalars": {
    "padding": {
      "type": "String",
      "allowed": "spacingType"
    },
    "margin": {
      "type": "String",
      "allowed": "spacingType"
    },
    "content": {
      "type": "componentRef",
      "isNull": true
    },
    "variableWidth": {
      "type": "String",
      "allowed": [
        "shrink-none",
        "grow-none"
      ]
    },
    "col": {
      "type": "Boolean",
      "defaultValue": false,
      "clientOnly": true
    },
    "bump": {
      "type": "String",
      "allowed": "positionX"
    },
    "columnAlignment": {
      "type": "String",
      "allowed": [
        "top",
        "middle",
        "bottom"
      ]
    },
    "size_$": {
      "type": "String",
      "allowed": "columnSize"
    },
    "order_$": {
      "type": "String",
      "allowed": "columnOrder"
    }
  },
  "events": {
    "bump": {
      "clientOnly": true
    }
  },
  "sample": {
    "nullifyRandomly": []
  },
  "escapeDynamicHtml": false
}