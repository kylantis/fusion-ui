{
  "enableTypeMerging": false,
  "collections": {
    "cssStyles": {
      "minLength": 0,
      "keyType": "String"
    }
  },
  "scalars": {
    "cssStyles.$_": {
      "type": "String"
    },
    "cssClass": {
      "type": "String",
      "clientOnly": true
    }
  },
  "behaviours": {
    "setTooltipText": {
      "parameters": {
        "title": {
          "type": "string"
        }
      }
    },
    "toggleCssClass": {
      "parameters": {
        "predicate": {
          "type": "boolean"
        },
        "className": {
          "type": "string"
        }
      }
    },
    "setHtmlAttribute": {
      "parameters": {
        "name": {
          "type": "string"
        },
        "value": {
          "type": "string"
        }
      }
    },
    "removeHtmlAttribute": {
      "parameters": {
        "name": {
          "type": "string"
        }
      }
    }
  },
  "sample": {
    "nullifyRandomly": []
  },
  "escapeDynamicHtml": false,
  "loadAfterCompile": false
}