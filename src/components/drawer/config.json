{
  "enableTypeMerging": false,
  "collections": {},
  "scalars": {
    "closeIcon": {
      "type": "Boolean",
      "defaultValue": true
    },
    "title": {
      "type": "Text",
      "isNull": false
    },
    "position": {
      "type": "String",
      "allowed": "positionX",
      "defaults": [
        "left"
      ]
    },
    "showByDefault": {
      "type": "Boolean",
      "defaultValue": true
    },
    "size": {
      "type": "String",
      "allowed": "layoutSize",
      "defaults": [
        "large"
      ]
    },
    "overlay": {
      "type": "Boolean",
      "defaultValue": true
    },
    "backdrop": {
      "type": "Boolean",
      "defaultValue": false
    },
    "toggleButton": {
      "type": "Boolean",
      "defaultValue": true
    }
  },
  "sample": {
    "nullifyRandomly": []
  },
  "escapeDynamicHtml": false
}