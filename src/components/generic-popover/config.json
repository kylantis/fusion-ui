{
  "collections": {},
  "scalars": {
    "feedbackTitle": {
      "type": "String",
      "defaults": [
        "Just a heads upâ€¦"
      ]
    },
    "feedbackState": {
      "type": "String",
      "allowed": [
        "warning",
        "error"
      ],
      "defaults": [
        null
      ]
    },
    "icon": {
      "type": "Icon",
      "isNull": false
    },
    "title": {
      "type": "String",
      "defaults": [
        "Prompt title"
      ]
    },
    "body": {
      "type": "componentRef",
      "defaults": [
        "Illustration"
      ]
    },
    "footer.theme": {
      "type": "String",
      "allowed": "colorTheme",
      "defaults": [
        null
      ]
    },
    "footer.leftButton": {
      "type": "Button",
      "isNull": false
    },
    "footer.actionLink": {
      "type": "Link",
      "isNull": false
    },
    "footer.rightButton": {
      "type": "Button",
      "isNull": false
    }
  },
  "sample": {
    "nullifyRandomly": [
      "footer.leftButton",
      "footer.actionLink",
      "footer.rightButton"
    ]
  },
  "escapeDynamicHtml": false,
  "behaviours": {
    "addFeedbackState": {
      "parameters": {
        "feedbackState": {
          "type": {
            "$ref": "feedbackState"
          }
        }
      }
    },
    "removeFeedbackState": {
      "parameters": {
        "feedbackState": {
          "type": {
            "$ref": "feedbackState"
          }
        }
      }
    }
  }
}