{
  "collections": {},
  "scalars": {
    "targetElement": {
      "type": "String",
      "clientOnly": true
    },
    "targetComponent": {
      "type": "componentRef",
      "clientOnly": true
    },
    "role": {
      "type": "String",
      "clientOnly": true
    },
    "label": {
      "type": "String"
    },
    "closeIcon": {
      "type": "Boolean",
      "defaultValue": true
    },
    "width": {
      "type": "String",
      "allowed": [
        "small",
        "medium",
        "large",
        "full-width"
      ],
      "defaults": [
        "large"
      ]
    },
    "maxHeight": {
      "type": "number",
      "defaultValue": 0
    },
    "dark": {
      "type": "Boolean",
      "defaultValue": false
    },
    "brand": {
      "type": "String",
      "allowed": [
        "top",
        "bottom",
        "left",
        "right"
      ]
    },
    "position": {
      "type": "String",
      "allowed": [
        "top-left",
        "top",
        "top-right",
        "bottom-right",
        "bottom",
        "bottom-left"
      ],
      "defaults": [
        "top"
      ]
    },
    "nubbin": {
      "type": "Boolean",
      "defaultValue": true
    }
  },
  "behaviours": {
    "setTargetComponent": {
      "parameters": {
        "ref": {
          "type": "String"
        }
      }
    }
  },
  "escapeDynamicHtml": false
}