{
  "enableTypeMerging": false,
  "collections": {
    "states": {
      "minLength": 2,
      "keyType": "buttonState",
      "singularType": "State"
    }
  },
  "scalars": {
    "type": {
      "type": "String",
      "allowed": [
        "neutral",
        "brand",
        "outline-brand",
        "destructive",
        "text-destructive",
        "success"
      ],
      "defaults": [
        "neutral"
      ]
    },
    "disabled": {
      "type": "Boolean",
      "defaultValue": false
    },
    "stretch": {
      "type": "Boolean",
      "defaultValue": false
    },
    "fullWidth": {
      "type": "Boolean",
      "defaultValue": false
    },
    "leftIcon": {
      "type": "ButtonIconSvg",
      "isNull": false
    },
    "rightIcon": {
      "type": "ButtonIconSvg",
      "isNull": true
    },
    "title": {
      "type": "String",
      "defaults": [
        "Shop Apparel"
      ]
    },
    "stateful": {
      "type": "Boolean",
      "defaultValue": true
    },
    "state": {
      "type": "String",
      "allowed": "buttonState",
      "defaults": [
        "selected"
      ]
    },
    "states.$_.leftIcon": {
      "type": "ButtonIconSvg",
      "isNull": false
    },
    "states.$_.rightIcon": {
      "type": "ButtonIconSvg",
      "isNull": false
    },
    "states.$_.title": {
      "type": "String",
      "defaults": [
        "Order now",
        "Create Profile",
        "Continue",
        "Global settings",
        "My Profile",
        "Delete Account",
        "Send gift",
        "Refresh"
      ]
    }
  },
  "sample": {
    "nullifyRandomly": []
  },
  "useCollectionLengthProperty": true,
  "escapeDynamicHtml": false
}