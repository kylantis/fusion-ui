{
  "enableTypeMerging": false,
  "collections": {
    "groups": {
      "minLength": 2,
      "singularType": "Group"
    },
    "groups_$.items": {
      "minLength": 5,
      "singularType": "Item"
    }
  },
  "scalars": {
    "length": {
      "type": "String",
      "allowed": [
        "five",
        "seven",
        "ten",
        "with-icon-five",
        "with-icon-seven",
        "with-icon-ten"
      ]
    },
    "fluid": {
      "type": "Boolean",
      "defaultValue": true
    },
    "type": {
      "type": "String",
      "allowed": "alignment"
    },
    "loading": {
      "type": "Boolean",
      "defaultValue": false
    },
    "entity": {
      "type": "boolean"
    },
    "groups_$.title": {
      "type": "String",
      "defaults": [
        "Group One",
        "Group Two",
        "Group Three",
        "Group Four"
      ]
    },
    "groups_$.loading": {
      "type": "Boolean"
    },
    "groups_$.items_$.disabled": {
      "type": "boolean",
      "defaultValue": false
    },
    "groups_$.items_$.icon": {
      "type": "Icon",
      "isNull": false
    },
    "groups_$.items_$.title": {
      "type": "string",
      "defaults": [
        "Edge Communications",
        "Alexandria Textiles Corp of America",
        "Express Logistics and Transport",
        "Reliance Communications",
        "Profiling Tools",
        "Flight Recorder Feature",
        "Global settings configuration",
        "My Profile",
        "Delete Account",
        "Send gift to Peer",
        "Refresh Private Folder",
        "Create Alliance Charter",
        "Order item",
        "Preview user realm",
        "Send message",
        "Detect multi-lingual channel",
        "Facilitate",
        "Incorporate",
        "Evaluate insurance risk",
        "Conceptualize startup paradigm",
        "Enable feature",
        "Generate",
        "Implement"
      ]
    },
    "groups_$.items_$.selected": {
      "type": "boolean",
      "defaultValue": true
    },
    "groups_$.items_$.focus": {
      "type": "boolean",
      "defaultValue": false
    },
    "groups_$.items_$.identifier": {
      "type": "string"
    },
    "groups_$.items_$.metaText": {
      "type": "string",
      "defaults": [
        "Durham, NC",
        "Alexandria, VA",
        "Cherry Creek, CO",
        "S. Valley, CA"
      ]
    },
    "groups_$.items_$.drillsIn": {
      "type": "boolean",
      "defaultValue": true
    },
    "stateIndex": {
      "type": "Number",
      "clientOnly": true
    },
    "allowMultipleStates": {
      "type": "boolean"
    },
    "searchTerm": {
      "type": "String",
      "clientOnly": true
    }
  },
  "events": {
    "itemsUpdate": {
      "clientOnly": true,
      "parameters": {
        "allItems": {
          "type": {
            "$ref": "groups_$.items"
          }
        },
        "addedItems": {
          "type": {
            "$ref": "groups_$.items"
          }
        },
        "removedItems": {
          "type": {
            "$ref": "groups_$.items"
          }
        }
      }
    }
  },
  "behaviours": {
    "pushState": {
      "parameters": {
        "state": {
          "type": "componentRef"
        }
      }
    },
    "addHighlight": {
      "parameters": {
        "searchTerm": {
          "type": "string"
        },
        "caseSensitive": {
          "type": "boolean"
        },
        "displaySearchTerm": {
          "type": "boolean"
        }
      }
    }
  },
  "sample": {
    "count": 5,
    "nullifyRandomly": [
      "groups_$.loading",
      "groups_$.items_$.icon",
      "items_$.icon",
      "groups_$.items_$.selected",
      "groups_$.items_$.metaText",
      "groups_$.items_$.drillsIn"
    ]
  },
  "escapeDynamicHtml": false,
  "hookAccessRate": 2
}