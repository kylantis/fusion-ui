
{{#each formValues as |val|}}
{{/each}}

<div class="slds-form" role="list">
  {{#each elements as |x y|}}
    <div class="slds-form__row">
      {{#each . as |x y|}}
        <div class="slds-form__item" role="listitem">
            {{component FormElement alias="."}}
            {{> .}}
        </div>
      {{/each}}
    </div>
  {{/each}}
</div>