{
  "enableTypeMerging": false,
  "collections": {
    "sections": {
      "minLength": 2,
      "singularType": "Section"
    },
    "sections_$.items": {
      "minLength": 5,
      "singularType": "Item"
    }
  },
  "scalars": {
    "truncateSize": {
      "type": "Number",
      "defaultValue": 3
    },
    "label": {
      "type": "String",
      "defaults": [
        "Vertical Nav"
      ]
    },
    "sections_$.id": {
      "type": "String"
    },
    "sections_$.name": {
      "type": "String",
      "defaults": [
        "Recent",
        "Folders",
        "Notes",
        "Tasks",
        "Profile",
        "Settings",
        "Infrastructure",
        "Users",
        "Catalog",
        "Prospects"
      ]
    },
    "sections_$.overflow.expanded": {
      "type": "Boolean"
    },
    "sections_$.items_$.active": {
      "type": "Boolean",
      "defaultValue": false
    },
    "sections_$.items_$.id": {
      "type": "String"
    },
    "sections_$.items_$.name": {
      "type": "String",
      "defaults": [
        "Recent",
        "Created by Me",
        "Created by others",
        "Recent Activity",
        "Private Reports",
        "Public Reports",
        "All Reports",
        "Merge Files",
        "Shared with Me",
        "Important Note",
        "View Notes",
        "Pending Tasks",
        "Users"
      ]
    },
    "sections_$.items_$.sectionId": {
      "type": "String",
      "clientOnly": true
    },
    "sections_$.items_$.rendered": {
      "type": "Boolean",
      "clientOnly": true
    },
    "sections_$.items_$.icon": {
      "type": "String",
      "allowed": "utilityIconName",
      "defaults": []
    },
    "sections_$.items_$.badge": {
      "type": "Badge",
      "isNull": false
    }
  },
  "behaviours": {
    "toggleOverflowArea": {
      "parameters": {
        "sectionIndex": {
          "type": "number"
        }
      }
    }
  },
  "events": {
    "itemRender": {
      "clientOnly": true
    }
  },
  "sample": {
    "nullifyRandomly": [
      "sections_$.items_$.icon",
      "sections_$.items_$.badge"
    ]
  },
  "escapeDynamicHtml": false
}