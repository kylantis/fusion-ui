{
  "enableTypeMerging": false,
  "collections": {
    "columns": {
      "minLength": 1,
      "keyType": "String"
    }
  },
  "scalars": {
    "wrap": {
      "type": "Boolean",
      "defaultValue": false
    },
    "columns_$": {
      "type": "GridElement",
      "isNull": false
    },
    "pullPadded": {
      "type": "String",
      "allowed": [
        "xxx-small",
        "xx-small",
        "x-small",
        "small",
        "medium",
        "large",
        "x-large",
        "xx-large"
      ],
      "defaults": [
        null
      ]
    },
    "gutterSize": {
      "type": "String",
      "allowed": [
        "x-small",
        "small",
        "medium",
        "large",
        "x-large",
        "direct-x-small",
        "direct-small",
        "direct-medium",
        "direct-large",
        "direct-x-large"
      ],
      "defaults": [
        "small"
      ]
    },
    "gridFlow": {
      "type": "String",
      "allowed": [
        "default",
        "vertical",
        "reverse",
        "vertical-reverse"
      ],
      "defaults": [
        "default"
      ]
    },
    "horizontalAlignment": {
      "type": "String",
      "allowed": [
        "center",
        "end",
        "space",
        "spread"
      ],
      "defaults": [
        null
      ]
    },
    "verticalAlignment": {
      "type": "String",
      "allowed": [
        "center",
        "end",
        "start"
      ],
      "defaults": [
        null
      ]
    },
    "boxPadding": {
      "type": "String",
      "allowed": [
        "xx-small",
        "x-small",
        "small"
      ],
      "defaults": [
        "x-small"
      ]
    }
  },
  "sample": {
    "nullifyRandomly": []
  },
  "escapeDynamicHtml": false
}