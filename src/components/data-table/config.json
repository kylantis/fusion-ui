{
  "enableTypeMerging": false,
  "collections": {
    "columns": {
      "minLength": 5,
      "keyType": "String",
      "defaultKeys": [
        "Name",
        "Related To",
        "From Address",
        "To Address",
        "Location",
        "Attendees",
        "When",
        "Opportunity Name",
        "Account Name",
        "Close Date",
        "Stage",
        "Confidence",
        "Amount",
        "Contact"
      ],
      "singularType": "ColumnConfig"
    },
    "rows": {
      "minLength": 10,
      "singularType": "Row"
    },
    "rows_$.cells": {
      "minLength": 5,
      "singularType": "Cell"
    }
  },
  "scalars": {
    "editable": {
      "type": "Boolean",
      "defaultValue": true
    },
    "resizableColumns": {
      "type": "Boolean",
      "defaultValue": true
    },
    "rowsSelectable": {
      "type": "Boolean",
      "defaultValue": true
    },
    "label": {
      "type": "String",
      "defaults": [
        null
      ]
    },
    "bordered": {
      "type": "Boolean",
      "defaultValue": true
    },
    "striped": {
      "type": "Boolean",
      "defaultValue": false
    },
    "borderColumns": {
      "type": "Boolean",
      "defaultValue": false
    },
    "labelledBy": {
      "type": "String",
      "defaults": [
        null
      ],
      "clientOnly": true
    },
    "inlineEdit": {
      "type": "Boolean",
      "defaultValue": true
    },
    "hasActions": {
      "type": "Boolean",
      "defaultValue": false,
      "clientOnly": true
    },
    "rows_$.id": {
      "type": "String",
      "defaults": [
        null
      ]
    },
    "rows_$.selected": {
      "type": "Boolean",
      "defaultValue": false
    },
    "rows_$.rowsSelectable": {
      "type": "Boolean",
      "defaultValue": true
    },
    "rows_$.actions": {
      "type": "Menu",
      "isNull": false
    },
    "rows_$.cells_$.component": {
      "type": "componentRef",
      "isNull": true,
      "defaults": [
        "Link"
      ]
    },
    "rows_$.cells_$.control": {
      "type": "FormElement",
      "isNull": false
    },
    "rows_$.cells_$.value.icon": {
      "type": "Icon",
      "isNull": true
    },
    "rows_$.cells_$.value.text": {
      "type": "String",
      "defaults": [
        "Action items",
        "Adoption",
        "Alignments",
        "Applications",
        "Architectures",
        "Bandwidth",
        "Benefits",
        "Best practices",
        "Channels",
        "4/12/20",
        "2/1/21",
        "3/2/17",
        "1/3/21",
        "5/10/22",
        "6/6/23",
        "7/8/24",
        "8/4/19",
        "15%",
        "25%",
        "30%",
        "95%",
        "80%",
        "70%",
        "50%",
        "63%",
        "$25,000,000",
        "$10,000,000",
        "$2,000",
        "$11,000",
        "$7,500",
        "$8,700",
        "$13,000",
        "$5,000,000"
      ]
    },
    "columns.$_.resizable": {
      "type": "Boolean",
      "defaultValue": true
    },
    "columns.$_.sortable": {
      "type": "Boolean",
      "defaultValue": true
    },
    "columns.$_.icon": {
      "type": "Icon",
      "isNull": false
    }
  },
  "behaviours": {
    "sortAscending": {
      "parameters": {
        "columnIndex": {
          "type": "number"
        }
      }
    },
    "sortDescending": {
      "parameters": {
        "columnIndex": {
          "type": "number"
        }
      }
    },
    "selectRow": {
      "parameters": {
        "rowIndex": {
          "type": "number"
        }
      }
    }
  },
  "sample": {
    "nullifyRandomly": [
      "columns.$_.icon",
      "rows_$.cells_$.control"
    ]
  },
  "useCollectionLengthProperty": true,
  "escapeDynamicHtml": false
}