{
  "collections": {
    "tabs": {
      "minLength": 5,
      "singularType": "Tab"
    }
  },
  "scalars": {
    "useWaffleIcon": {
      "type": "Boolean",
      "defaultValue": true
    },
    "appName": {
      "type": "String",
      "defaults": [
        "Kylantis Studio"
      ]
    },
    "tabs_$.content": {
      "type": "componentRef",
      "defaults": [
        "ActivityTimeline"
      ]
    },
    "useDivider": {
      "type": "Boolean",
      "defaultValue": true
    },
    "hideOnItemClick": {
      "type": "Boolean",
      "defaultValue": true
    },
    "expandOnHover": {
      "type": "Boolean",
      "defaultValue": false
    },
    "tabs_$.identifier": {
      "type": "String",
      "defaults": []
    },
    "tabs_$.title": {
      "type": "String",
      "defaults": [
        "DashboardPage",
        "UsersPage",
        "InvoicePage",
        "SettingsPage",
        "OrdersPage",
        "ListingsPage"
      ]
    },
    "tabs_$.assistiveText": {
      "type": "String",
      "defaults": []
    },
    "tabs_$.closeable": {
      "type": "Boolean",
      "defaultValue": true
    },
    "icon": {
      "type": "Icon",
      "isNull": false
    },
    "tabs_$.subMenu": {
      "type": "Menu",
      "isNull": false
    },
    "tabs_$.isActive": {
      "type": "Boolean",
      "defaultValue": false
    }
  },
  "escapeDynamicHtml": false,
  "sample": {
    "nullifyRandomly": [
      "tabs_$.content"
    ]
  }
}