{
  "enableTypeMerging": false,
  "collections": {},
  "scalars": {
    "showByDefault": {
      "type": "Boolean",
      "defaultValue": true
    },
    "bodyPadding": {
      "type": "String",
      "allowed": "spacingType",
      "defaults": [
        "around_medium"
      ]
    },
    "size": {
      "type": "String",
      "allowed": [
        "small",
        "medium",
        "large"
      ],
      "defaults": [
        "small"
      ]
    },
    "cssClass": {
      "type": "String",
      "defaults": [
        null
      ]
    },
    "tabIndex": {
      "type": "Number",
      "defaultValue": -1
    },
    "directional": {
      "type": "Boolean",
      "defaultValue": false
    },
    "closeIcon": {
      "type": "Boolean",
      "defaultValue": false
    }
  },
  "clientOnly": true,
  "sample": {
    "nullifyRandomly": []
  },
  "useCollectionLengthProperty": false,
  "escapeDynamicHtml": false
}